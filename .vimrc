" au BufRead,BufNewFile *.fountain set filetype=fountain
" ignore case when /searching
set ignorecase 
set number
syntax on
set ruler
set mouse=a
" things that clutter git
set nobackup
set noswapfile
" ident
set tabstop=4 shiftwidth=4 expandtab
" f10 to go to end of line (so you have 0 and F10)
nmap <F10> $
